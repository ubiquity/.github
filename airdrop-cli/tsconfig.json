{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false,
    "noImplicitReturns": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "stripInternal": true,
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "newLine": "LF",
    "outDir": "dist",
    "plugins": [],
  },
  "include": [
    "src",
    "src/commands/unspent.ts",
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}